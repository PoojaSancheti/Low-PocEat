[run]
source = .
omit = 
    */tests/*
    */migrations/*
    */__init__.py
    */settings/*
    */manage.py
    */wsgi.py
    */asgi.py
    */urls.py

[report]
# Regexes for lines to exclude from consideration
exclude_lines =
    # Have to re-enable the standard pragma
    pragma: no cover

    # Don't complain about missing debug-only code:
    def __repr__
    if self\.logger\.debug
    raise NotImplementedError
    raise ImportError
    if 0:
    if __name__ == .__main__.:
    pass

    # Don't complain if tests don't hit defensive assertion code:
    raise AssertionError
    raise NotImplementedError

    # Don't complain if non-runnable code isn't run:
    if 0:
    if __name__ == .__main__.:

    # Don't complain about abstract methods that aren't called in tests
    @(abc\.)?abstractmethod

    # Ignore Django's DEBUG setting
    settings\.DEBUG

    # Ignore coverage of error handling code
    except ImportError:

[html]
directory = htmlcov

[xml]
output = coverage.xml
